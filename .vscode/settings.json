{
  "java.compile.nullAnalysis.mode": "automatic",
  "java.format.settings.url": ".vscode/java-formatter.xml",
  "java.configuration.updateBuildConfiguration": "automatic",
  "maven.view": "flat",
  "jdk.runConfig.vmOptions": "-Xms256m -Xmx512m",
  "jdk.advanced.disable.nbjavac": false,
  "java.debug.settings.onBuildFailureProceed": true,
  "maven.terminal.favorites": [
    {
      "command": "clean compile",
      "name": "Maven Clean compile"
    },
    {
      "command": "spotless:apply",
      "name": "Spotless Apply"
    },
    {
      "command": "spring-boot:run",
      "debug": false
    }
  ],
  "spring.tools.properties.replace-converted-file": false,
  "boot-java.highlight-copilot-codelens.on": true
}
